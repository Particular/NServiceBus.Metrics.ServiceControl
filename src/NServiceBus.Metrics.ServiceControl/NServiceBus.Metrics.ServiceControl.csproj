<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net452;netstandard2.0</TargetFrameworks>
    <OutputPath>..\..\binaries\</OutputPath>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="NServiceBus" Version="7.0.0-beta0007" />
    <PackageReference Include="NServiceBus.Metrics" Version="3.0.0-rc0002" />
    <PackageReference Include="ServiceControl.Monitoring.Data" Version="1.1.1-source-package0001" PrivateAssets="All" />
  </ItemGroup>

</Project>