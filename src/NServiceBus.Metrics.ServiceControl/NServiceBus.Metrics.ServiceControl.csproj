<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\NServiceBus.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NServiceBus" Version="[5.2.23, 6.0.0)" />
    <PackageReference Include="ServiceControl.Monitoring.Data" Version="[2.1.1, 3.0.0)" />
    <PackageReference Include="Particular.Packaging" Version="0.1.0" PrivateAssets="All" />
  </ItemGroup>

  <PropertyGroup>
    <Description>The component NServiceBus.Metrics.ServiceControl enables sending monitoring data gathered with NServiceBus.Metrics to an instance of ServiceControl.Monitoring service.</Description>
  </PropertyGroup>

</Project>